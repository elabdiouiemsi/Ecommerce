@model IEnumerable<Category>

@{
    ViewData["Title"] = "Index";
}


<h2>Liste des catégories</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nom</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model)
        {
            <tr>
                <td>@category.CategoryId</td>
                <td>@category.Nom</td>
            </tr>
        }
    </tbody>
</table>
